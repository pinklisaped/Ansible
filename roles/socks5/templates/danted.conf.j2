logoutput: stdout
errorlog: stderr
internal: {{ ansible_default_ipv4['interface'] }} port = {{ socks5_port }}
external: {{ ansible_default_ipv4['interface'] }}

socksmethod: username
user.privileged: root
user.unprivileged: nobody

client pass {
    from: 0.0.0.0/0 to: 0.0.0.0/0
    log: {{ socks5_loglevel }}
}

socks pass {
    from: 0.0.0.0/0 to: 0.0.0.0/0
    command: connect
    log: {{ socks5_loglevel }}
    socksmethod: username
}